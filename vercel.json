{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "js/*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/register", "dest": "/register.html" },
    { "src": "/dashboard", "dest": "/admin-dashboard.html" },
    { "src": "/admin-login", "dest": "/admin-login.html" },
    { "src": "/admin-fizzx", "dest": "/admin-register.html" },
    { "src": "/js/(.*)", "dest": "/js/$1" }
  ]
}
